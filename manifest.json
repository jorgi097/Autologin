{
    "manifest_version": 3,
    "name": "Auto-Login",
    "version": "1.0",
    "description": "Entrar autom√°ticamente a mi cuenta al abrir tickets.",
    "permissions": [
        "scripting",
        "tabs",
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "js": [
                "content-script.js"
            ],
            "matches": [
                "http://tickets.corporativoes.com/*"
            ],
            "all_frames": true,
            "run_at": "document_idle",
            "world": "MAIN"
        }
    ],
    "host_permissions": [
        "http://tickets.corporativoes.com/*"
    ],
    "action": {
        "default_title": "Autologin",
        "default_popup": "popup/popup.html"
    }
}
